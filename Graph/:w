
public class Edge {

  public Node src;
  public Node dst;
  public int weight;

  public Edge(Node src, Node dst, int weight) {
    this.src = src;
    this.dst = dst;
    this.weight = weight;
  }

  public Edge(Node src, Node dst, int weight) {
    this.src = src;
    this.dst = dst;
    this.weight = weight;
  }

}
